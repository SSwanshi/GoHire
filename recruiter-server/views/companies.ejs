<%- include('layout/header') %>

<h2>Here is your list of recently added Companies</h2>

<table>
    <thead>
        <tr>
            <th>Logo</th>
            <th>Name</th>
            <th>Date</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        <% companies.forEach(company => { %>
            <tr>
                <td><img src="<%= company.logo %>" alt="<%= company.name %>" width="40"></td>
                <td><%= company.name %></td>
                <td><%= company.date %></td>
                <td> 
                    <button>...</button>
                </td>
            </tr>
        <% }) %>
    </tbody>
</table>
<br><br>

<%- include('layout/footer') %>

